[
  {
    "dnsSearchDomains": null,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/dispute-tools-staging",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "entryPoint": [],
    "portMappings": [
      {
        "hostPort": 8080,
        "protocol": "tcp",
        "containerPort": 8080
      }
    ],
    "command": [],
    "linuxParameters": null,
    "cpu": 1024,
    "environment": [
      {
        "name": "SSO_ENDPOINT",
        "value": "${sso_endpoint}"
      },
      {
        "name": "SSO_SECRET",
        "value": "${sso_secret}"
      },
      {
        "name": "JWT_SECRET",
        "value": "${jwt_secret}"
      },
      {
        "name": "SSO_COOKIE_NAME",
        "value": "${cookie_name}"
      },
      {
        "name": "NODE_ENV",
        "value": "${environment}"
      },
      {
        "name": "EMAIL_CONTACT",
        "value": "${contact_email}"
      },
      {
        "name": "EMAIL_NO_REPLY",
        "value": "${sender_email}"
      },
      {
        "name": "EMAIL_DISPUTES_BCC",
        "value": "${disputes_bcc_address}"
      },
      {
        "name": "EMAIL_HOST",
        "value": "${smtp_host}"
      },
      {
        "name": "EMAIL_PORT",
        "value": "${smtp_port}"
      },
      {
        "name": "EMAIL_SECURE",
        "value": "true"
      },
      {
        "name": "EMAIL_AUTH",
        "value": "${smtp_user}"
      },
      {
        "name": "EMAIL_PASS",
        "value": "${smtp_pass}"
      },
      {
        "name": "LOGGLY_KEY",
        "value": "${loggly_api_key}"
      },
      {
        "name": "STRIPE_PRIVATE",
        "value": "${stripe_private}"
      },
      {
        "name": "STRIPE_PUBLISHABLE",
        "value": "${stripe_publishable}"
      },
      {
        "name": "GMAPS_KEY",
        "value": "${google_maps_api_key}"
      },
      {
        "name": "AWS_UPLOAD_BUCKET",
        "value": "tds-tools-${environment}"
      },
      {
        "name": "AWS_ACCESS_KEY_ID",
        "value": "${access_key_id}"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "value": "${secret_access_key}"
      },
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "${bucket_region}"
      },
      {
        "name": "DB_CONNECTION_STRING",
        "value": "${db_connection_string}"
      },
      {
        "name": "DB_POOL_MIN",
        "value": "${db_pool_min}"
      },
      {
        "name": "DB_POOL_MAX",
        "value": "${db_pool_max}"
      }
    ],
    "ulimits": null,
    "dnsServers": null,
    "mountPoints": null,
    "workingDirectory": null,
    "dockerSecurityOptions": null,
    "memoryReservation": "",
    "volumesFrom": null,
    "image": "${repository_url}:latest",
    "disableNetworking": null,
    "healthCheck": null,
    "essential": true,
    "links": [],
    "hostname": null,
    "extraHosts": null,
    "user": null,
    "readonlyRootFilesystem": null,
    "dockerLabels": null,
    "privileged": null,
    "name": "dispute-tools"
  }
]
