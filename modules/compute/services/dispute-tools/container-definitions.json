[
  {
    "memory": 300,
    "portMappings": [
      {
        "hostPort": 80,
        "containerPort": 80
      }
    ],
    "networkMode": "awsvpc",
    "command": [],
    "environment": [
      {
        "name": "SSO_ENDPOINT",
        "value": "${sso_endpoint}"
      },
      {
        "name": "SSO_SECRET",
        "value": "${sso_secret}"
      },
      {
        "name": "JWT_SECRET",
        "value": "${jwt_secret}"
      },
      {
        "name": "SSO_COOKIE_NAME",
        "value": "${cookie_name}"
      },
      {
        "name": "NODE_ENV",
        "value": "${environment}"
      },
      {
        "name": "EMAIL_CONTACT",
        "value": "${contact_email}"
      },
      {
        "name": "EMAIL_NO_REPLY",
        "value": "${sender_email}"
      },
      {
        "name": "EMAIL_DISPUTES_BCC",
        "value": "${disputes_bcc_address}"
      },
      {
        "name": "EMAIL_HOST",
        "value": "${smtp_host}"
      },
      {
        "name": "EMAIL_PORT",
        "value": "${smtp_port}"
      },
      {
        "name": "EMAIL_SECURE",
        "value": "${smtp_secure}"
      },
      {
        "name": "EMAIL_AUTH",
        "value": "${smtp_user}"
      },
      {
        "name": "EMAIL_PASS",
        "value": "${smtp_pass}"
      },
      {
        "name": "LOGGLY_KEY",
        "value": "${loggly_api_key}"
      },
      {
        "name": "STRIPE_PRIVATE",
        "value": "${stripe_private}"
      },
      {
        "name": "STRIPE_PUBLISHABLE",
        "value": "${stripe_publishable}"
      },
      {
        "name": "GMAPS_KEY",
        "value": "${google_maps_api_key}"
      },
      {
        "name": "AWS_UPLOAD_BUCKET",
        "value": "tds-tools-${environment}"
      },
      {
        "name": "AWS_ACCESS_KEY_ID",
        "value": "${aws_access_id}"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "value": "${aws_access_secret}"
      },
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "${aws_region}"
      },
      {
        "name": "DB_CONNECTION_STRING",
        "value": "${db_connection_string}"
      },
      {
        "name": "DB_POOL_MIN",
        "value": "${db_pool_min}"
      },
      {
        "name": "DB_POOL_MAX",
        "value": "${db_pool_max}"
      }
    ],
    "image": "${image_name}",
    "name": "tds-dispute-tools"
  }
]
